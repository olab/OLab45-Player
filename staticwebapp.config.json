{
  "routes": [
    {
      "route": "/index.html",
      "redirect": "/player"
    },
    {
      "route": "/",
      "redirect": "/player"
    }
  ],
  "navigationFallback": {
    "rewrite": "/player/index.html"
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
  }  
}
